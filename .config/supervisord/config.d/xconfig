[program:compton]
command=compton
autorestart=true

[program:dunst]
command=dunst
autorestart=true

[program:xflux]
command=xflux -l 52 -g 0 -nofork
autorestart=true
redirect_stderr=true

[program:xautolock]
command=xautolock -time 10 -locker laptop-lock -killtime 20 -killer "on_ac_power || laptop-smartsleep" -secure
autorestart=true
