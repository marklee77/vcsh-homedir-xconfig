[program:i3]
command=i3
priority=10
autorestart=true

[program:compton]
command=compton
autorestart=true

[program:dunst]
command=dunst
autorestart=true

[program:gvfs-automount]
command=gvfs-automount
autorestart=true

[program:pulseaudio
command=pulseaudio --daemonize=false --log-level=3 --log-target=stderr
autorestart=true

[program:unclutter]
command=unclutter -idle 5 -root
autorestart=true

[program:syndaemon]
command=syndaemon -i 1
autorestart=true

[program:urxvtd]
command=urxvtd
autorestart=true

[program:xautolock]
command=xautolock -time 5 -locker laptop-lock -killtime 20 -killer "on_ac_power || laptop-smartsleep" -secure
autorestart=true
