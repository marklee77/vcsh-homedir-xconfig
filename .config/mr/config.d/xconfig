[${XDG_CONFIG_HOME:-$HOME/.config}/vcsh/repo.d/xconfig.git]
checkout = vcsh clone https://gitlab.stillwell.me/mark/vcsh-homedir-xconfig.git xconfig
setpush  = vcsh xconfig remote set-url --add --push origin git@stillwell.me:mark/vcsh-homedir-xconfig.git
           vcsh xconfig remote set-url --add --push origin git@github.com:marklee77/vcsh-homedir-xconfig.git
           vcsh xconfig remote set-url --add --push origin git@bitbucket.org:marklee77/vcsh-homedir-xconfig.git

